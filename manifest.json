            "js": ["src/shared/scripts/shared.envGlobalsSetup.js","src/vendor/jquery/dist/jquery.min.js","src/vendor/handlebars/dist/handlebars.min.js","src/shared/scripts/shared.utils.js","src/vendor/chrome-extension-message-relay-v3/dist/message_relay.full.prod.js","src/shared/scripts/shared.loggableClass.js","src/shared/scripts/shared.eventableClass.js","src/content/scripts/content.logger.js","src/content/scripts/content.contentUtils.js","src/shared/scripts/shared.storeUtils.js","src/shared/scripts/shared.frameBridge.js","src/content/scripts/content.liveReload.js","src/shared/scripts/shared.stateBaseClass.js","src/content/scripts/content.state.js","src/content/scripts/content.configTemplates.js","src/content/scripts/content.templates.js","src/content/scripts/content.uiGeneral.js","src/content/scripts/content.pageFeatureBaseClass.js","src/content/scripts/content.clipboard.js","src/content/scripts/content.pageFeatureBaseClass.js","src/content/scripts/content.extract.js","src/content/scripts/content.pageButtons.js","src/content/scripts/content.xcomShim.js","src/content/scripts/content.extractInit.js"],
            "css": ["src/content/css/content.extract.css"]
