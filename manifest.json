{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Indeed Recruiter Extension",
    "short_name": "Indeed Recruiter Extension",
    "version": "65.0",
    "manifest_version": 3,
    "description": "A free browser extension that helps recruiters transfer candidate and resume data from Indeed to their ATS with just a few clicks.",
    "homepage_url": "https://employers.indeed.com/",
    "key": "",
    "icons": {
        "16": "images/indeed_icon_128.png",
        "48": "images/indeed_icon_128.png",
        "128": "images/indeed_icon_128.png"
    },
    "action": {
        "default_icon": {
            "16": "images/indeed_icon_38.png",
            "38": "images/indeed_icon_38.png"
        },
        "default_title": "Indeed Recruiter Extension",
        "default_popup": "src/popup/popup.html"
    },
    "background": {
        "service_worker": "/manifest/manifest_service_worker_bundle.js"
    },
    "web_accessible_resources": [
        {
            "matches": [
                "https://calendar.google.com/*",
                "https://employers.indeed.com/*",
                "https://employers.qa.indeed.net/*",
                "https://resumes.indeed.com/*",
                "https://resumes.sandbox.qa.indeed.net/*",
                "https://roz.sandbox.qa.indeed.net/*"
            ],
            "resources": [
                "/src/*",
                "/images/*",
                "/manifest/*",
                "files.manifest.json"
            ]
        }
    ],
    "externally_connectable": {
        "matches": [
            "https://my.irx.indeed.com/*"
        ]
    },
    "permissions": [
        "storage",
        "unlimitedStorage",
        "tabs",
        "alarms",
        "cookies"
    ],
    "host_permissions": [
        "*://indeed.net/*",
        "*://*.indeed.net/*",
        "*://indeed.com/*",
        "*://*.indeed.com/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self' http://localhost:9000 http://localhost:8088/; object-src 'self'; connect-src https://* 'self'"
    },
    "incognito": "spanning",
    "content_scripts": [
        {
            "matches": [
                "https://calendar.google.com/calendar/*"
            ],
            "js": ["src/shared/scripts/shared.envGlobalsSetup.js","src/vendor/jquery/dist/jquery.min.js","src/vendor/handlebars/dist/handlebars.min.js","src/shared/scripts/shared.utils.js","src/vendor/chrome-extension-message-relay-v3/dist/message_relay.full.prod.js","src/shared/scripts/shared.loggableClass.js","src/shared/scripts/shared.eventableClass.js","src/content/scripts/content.logger.js","src/content/scripts/content.contentUtils.js","src/shared/scripts/shared.storeUtils.js","src/shared/scripts/shared.frameBridge.js","src/content/scripts/content.liveReload.js","src/shared/scripts/shared.stateBaseClass.js","src/content/scripts/content.state.js","src/content/scripts/content.configTemplates.js","src/content/scripts/content.templates.js","src/content/scripts/content.uiGeneral.js","src/content/scripts/content.pageFeatureBaseClass.js","src/content/scripts/interview_sites/content.interviewSite.base.js","src/content/scripts/interview_sites/content.interviewSite.gcal.js","src/content/scripts/content.interviews.js","src/content/scripts/content.clipboard.js","src/content/scripts/content.interviews.calendar.js"],
            "css": ["src/content/css/content.interview.css"]
        },
        {
            "matches": [
                "https://employers.indeed.com/c#candidates/view?*",
                "https://employers.qa.indeed.net/c#candidates/view?*",
                "https://employers.qa.indeed.net/candidates/view?*",
                "https://employers.indeed.com/candidates/view?*"
